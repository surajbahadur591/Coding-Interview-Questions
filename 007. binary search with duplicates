binary search with duplicates : find first occurance of target


int binarySearch ( int arr[], int target){
      int start =0 , end = arr.length-1;
      while( start <=end ){
                int mid = start + (end - start)/2;

            if (arr[mid] > target || (a[mid] == target && mid >0 && a[mid-1]==target)) {
                  end = mid-1;
            }
            else (a[mid] < target ){
                  start = mid +1;
            }
            else return mid;
      }
      return -1;
}


